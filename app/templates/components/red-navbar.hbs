{{#bs-navbar backgroundColor="white" type="light" position="sticky-top" fluid=false as |navbar|}}
  <div class="navbar-header">
    {{navbar.toggle}}

    {{#link-to "application" class="navbar-brand"}}
      <img
        src="logo-brand.png"
        class="img-responsive"
        title={{nombreApp}}
        alt="Congreso"
        height="70">
    {{/link-to}}
  </div>

  {{#navbar.content}}
    {{#navbar.nav justified=true type="pills" class="ml-auto" as |nav|}}
      {{#each model.navbarLinks as |link|}}
        {{#nav.item active=false class="mx-4"}}{{#link-to link.route}}{{link.title}}{{/link-to}}{{/nav.item}}
      {{/each}}

      {{#nav.item active=false class="mx-4"}}{{#link-to "partidos"}}Presidencia{{/link-to}}{{/nav.item}}

      {{#nav.item active=false class="mx-4"}}{{#link-to "partidos"}}Alcald√≠a{{/link-to}}{{/nav.item}}

      {{#nav.item active=false class="mx-4"}}{{#link-to "partidos"}}Congreso{{/link-to}}{{/nav.item}}

      {{#nav.item active=false class="mx-4"}}{{#link-to "partidos"}}Parlacem{{/link-to}}{{/nav.item}}

    {{/navbar.nav}}
  {{/navbar.content}}
{{/bs-navbar}}
